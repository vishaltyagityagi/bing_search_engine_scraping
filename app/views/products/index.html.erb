<p id="notice"><%= notice %></p>

<h1>Listing Search Result</h1>

<table>
  <thead>
    <tr>
      <!-- <th>Search</th> -->
      
      <th>Searched Title</th>
      <th>Action</th>



      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @products.present? %>
    <% @products.each do |product| %>
        <% @title = JSON.parse(product.title) %>
        <% @description = JSON.parse(product.description) %>
        <% @url = JSON.parse(product.url) %>

        <tr>
        <td><%= product.search %></td>
          <td><%= link_to 'Show', product_path(product) %></td>
        </tr>

        
        
    <% end  %>
  <% end %>
  </tbody>
</table>

<br>
        

<%= link_to 'New Search', new_product_path %>
